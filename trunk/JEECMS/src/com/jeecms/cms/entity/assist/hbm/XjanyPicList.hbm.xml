<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.jeecms.cms.entity.assist">
	<class name="XjanyPicList" table="jc_piclist_xjany">
		<meta attribute="sync-DAO">false</meta>
		<id name="id" type="integer" column="id"><generator class="identity"/></id>
		<property name="name" column="name" type="string" not-null="true" length="150"/>
		<property name="url" column="url" type="string" not-null="true" length="255"/>
		<property name="pic" column="pic" type="string" not-null="false" length="150"/>
	</class>
</hibernate-mapping>