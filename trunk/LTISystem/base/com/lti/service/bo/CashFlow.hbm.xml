<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.lti.service.bo">
	<class
		name="CashFlow"
		table="ltisystem_cashflow"
	>
		<id
			name="ID"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="native"/>
		</id>
		<property
			name="Symbol"
			column="Symbol"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Date"
			column="Date"
			type="timestamp"
			not-null="false"
			length="19"
		/>
		<property
			name="Year"
			column="Year"
			type="integer"
			not-null="false"
			length="11"
		/>	
		<property
			name="Quarter"
			column="Quarter"
			type="integer"
			not-null="false"
			length="11"
		/>	
		<property
			name="DeprAmort"
			column="DeprAmort"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="AdjNetInc"
			column="AdjNetInc"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="OtherOperCash"
			column="OtherOperCash"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="CapExp"
			column="CapExp"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="Investment"
			column="Investment"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="OtherInvCash"
			column="OtherInvCash"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="Dividend"
			column="Dividend"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="StockSalePur"
			column="StockSalePur"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="NetBorrow"
			column="NetBorrow"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="OtherFinCash"
			column="OtherFinCash"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>		
		<property
			name="CurrencyAdj"
			column="CurrencyAdj"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="OperCF"
			column="OperCF"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="FinCF"
			column="FinCF"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="InvCF"
			column="InvCF"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>

	</class>	
</hibernate-mapping>