<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.lti.service.bo">
	<class
		name="YearlyIncomeStatement"
		table="ltisystem_incomestatement_yearly"
	>
		<id
			name="ID"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="native"/>
		</id>

		<property
			name="Symbol"
			column="Symbol"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="EndDate"
			column="EndDate"
			type="timestamp"
			not-null="false"
			length="19"
		/>
		<property
			name="Year"
			column="Year"
			type="integer"
			not-null="false"
			length="11"
		/>	
		<property
			name="Revenue"
			column="Revenue"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="COGS"
			column="COGS"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="RD"
			column="RD"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="IntExp"
			column="IntExp"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="IncomeTax"
			column="IncomeTax"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>		
		<property
			name="PreTaxInc"
			column="PreTaxInc"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		
		<property
			name="AfterTaxInc"
			column="AfterTaxInc"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="GrossProfit"
			column="GrossProfit"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="TotalOperExp"
			column="TotalOperExp"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="OperInc"
			column="OperInc"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="NetInc"
			column="NetInc"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>

		<property
			name="Shares"
			column="Shares"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="Floats"
			column="Floats"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>	
		<property
			name="InstitutionHolder"
			column="InstitutionHolder"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>	

											
	</class>	
</hibernate-mapping>