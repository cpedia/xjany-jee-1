<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.lti.service.bo">
	<class
		name="User"
		table="ltisystem_user"
	>
		<id
			name="ID"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="native"/>
		</id>

		<property
			name="UserName"
			column="USERNAME"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Password"
			column="PASSWORD"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Enabled"
			column="ENABLED"
			type="java.lang.Boolean"
			not-null="false"
		/>
		<property
			name="EMail"
			column="EMAIL"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Description"
			column="description"
			type="string"
			not-null="false"
		/>
		<property
			name="Telephone"
			column="TELEPHONE"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="QQ"
			column="QQ"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="Address"
			column="ADDRESS"
			type="string"
			not-null="false"
			length="100"
		/>
			<property
			name="CharCode"
			column="CHARCODE"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="PlanIDNum"
			column="PlanIDNum"
			type="java.lang.Integer"
			not-null="false"
		/>
		
		<property
			name="strategyCount"
			column="strategyCount"
			type="java.lang.Integer"
			not-null="false"
		/>
		<property
			name="portfolioCount"
			column="portfolioCount"
			type="java.lang.Integer"
			not-null="false"
		/>
		<property
			name="fundtableCount"
			column="fundtableCount"
			type="java.lang.Integer"
			not-null="false"
		/>
		<property
			name="firstName"
			column="first_name"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="lastName"
			column="last_name"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="addressState"
			column="address_state"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="addressCountry"
			column="address_country"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="addressCity"
			column="address_city"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="company"
			column="company"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="license"
			column="license"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="addressZip"
			column="address_zip"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="logo"
			column="logo"
			type="string"
			not-null="false"
		/>
		<property
			name="ThirdParty"
			column="ThirdParty"
			type="string"
			not-null="false"
		/>
		<property
			name="createdDate"
			column="createddate"
			type="java.util.Date"
			not-null="false"
		/>
		<property
			name="InviteCodeID"
			column="InviteCodeID"
			type="java.lang.Long"
			not-null="false"
		/>
	</class>	
</hibernate-mapping>