<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.lti.service.bo">
	<class
		name="CachePortfolioItem"
		table="cache_group_portfolio"
	>
		<composite-id>
			<key-property
				name="GroupID"
				column="GroupID"
				type="java.lang.Long"
			/>
			<key-property
				name="RoleID"
				column="RoleID"
				type="java.lang.Long"
			/>
			<key-property
				name="PortfolioID"
				column="PortfolioID"
				type="java.lang.Long"
			/>
		</composite-id>

		
		<property
			name="AR1"
			column="AR1"
			type="java.lang.Double"
			not-null="false"
		/>
		<property
			name="AR3"
			column="AR3"
			type="java.lang.Double"
			not-null="false"
		/>
		<property
			name="AR5"
			column="AR5"
			type="java.lang.Double"
			not-null="false"
		/>
		<property
			name="SharpeRatio1"
			column="SharpeRatio1"
			type="java.lang.Double"
			not-null="false"
		/>
		<property
			name="SharpeRatio3"
			column="SharpeRatio3"
			type="java.lang.Double"
			not-null="false"
		/>
		<property
			name="SharpeRatio5"
			column="SharpeRatio5"
			type="java.lang.Double"
			not-null="false"
		/>
		<property
			name="UserID"
			column="UserID"
			type="java.lang.Long"
			not-null="false"
		/>
		<property
			name="MainStrategyID"
			column="MainStrategyID"
			type="java.lang.Long"
			not-null="false"
		/>
		<property
			name="PortfolioName"
			column="PortfolioName"
			type="java.lang.String"
			not-null="false"
		/>
		<property
			name="UserName"
			column="UserName"
			type="java.lang.String"
			not-null="false"
		/>
		<property
			name="EndDate"
			column="EndDate"
			type="timestamp"
			not-null="false"
			length="19"
		/>		
		<property
			name="LastTransactionDate"
			column="LastTransactionDate"
			type="timestamp"
			not-null="false"
			length="19"
		/>		
		<property
			name="Type"
			column="Type"
			type="java.lang.Long"
			not-null="false"
		/>
		<property
			name="State"
			column="State"
			type="java.lang.Integer"
			not-null="false"
		/>
		
	</class>	
</hibernate-mapping>