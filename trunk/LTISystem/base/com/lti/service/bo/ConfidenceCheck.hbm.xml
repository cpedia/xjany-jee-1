<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.lti.service.bo">
	<class
		name="ConfidenceCheck"
		table="ltisystem_confidence_check"
	>
	<id
			name="ID"
			type="java.lang.Long"
			column="ID"
		>
		<generator class="native"/>
		</id>
		<property
			name="PortfolioID"
			column="PortfolioID"
			type="java.lang.Long"
			not-null="false"			
		/>
		<property
			name="StrategyID"
			column="StrategyID"
			type="java.lang.Long"
			not-null="false"			
		/>
		<property
			name="RuleName"
			column="RuleName"
			type="string"
			not-null="false"
			length="45"
		/>
		<property
			name="SampleSize"
			column="SampleSize"
			type="java.lang.Long"
			not-null="false"
		/>
		<property
			name="Mean"
			column="Mean"
			type="java.lang.Double"
			not-null="false"
		/>
		<property
			name="Variance"
			column="Variance"
			type="java.lang.Double"
			not-null="false"
		/>
		<property
			name="AboveMeanPossibility"
			column="AboveMeanPossibility"
			type="java.lang.Double"
			not-null="false"
		/>
		<property
			name="MaxReturnUnderSampleProportion5"
			column="MaxReturnUnderSampleProportion5"
			type="java.lang.Double"
			not-null="false"
		/>
		<property
			name="MaxReturnUnderSampleProportion10"
			column="MaxReturnUnderSampleProportion10"
			type="java.lang.Double"
			not-null="false"
		/>
		<property
			name="MaxReturnUnderSampleProportion15"
			column="MaxReturnUnderSampleProportion15"
			type="java.lang.Double"
			not-null="false"
		/>
		<property
			name="MaxReturnUnderSampleProportion20"
			column="MaxReturnUnderSampleProportion20"
			type="java.lang.Double"
			not-null="false"
		/>
		<property
			name="MaxReturnUnderSampleProportion30"
			column="MaxReturnUnderSampleProportion30"
			type="java.lang.Double"
			not-null="false"
		/>
		<property
			name="MaxReturnUnderSampleProportion40"
			column="MaxReturnUnderSampleProportion40"
			type="java.lang.Double"
			not-null="false"
		/>
		<property
			name="MaxReturnUnderSampleProportion50"
			column="MaxReturnUnderSampleProportion50"
			type="java.lang.Double"
			not-null="false"
		/>
		<property
			name="MaxReturnUnderSampleProportion60"
			column="MaxReturnUnderSampleProportion60"
			type="java.lang.Double"
			not-null="false"
		/>
		
	</class>
</hibernate-mapping>