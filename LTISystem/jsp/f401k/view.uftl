function save_for_mce(ed){
	var id=$(ed).attr('id');
	if (tinyMCE.getInstanceById(id)){
		tinyMCE.execCommand('mceFocus', false, id);                    
		tinyMCE.execCommand('mceRemoveControl', false, id);
		$('#'+id+'_span').html($('#'+id).val());
		$('#'+id+'_span').show();
		$('#'+id+'_tag').html('*');
		$('#'+id).hide();
		$('#'+id+'_hidden').val($('#'+id).val());
	} 
	editing=false;
}


function add_processbar_css(element, score){
	score = score*100;
	$('#'+element).progressbar({
			value: score
	});
	$('#'+element).css({ 'background': 'white' , 'height' : '18px'});
	$('#'+element).removeClass('ui-corner-all');
	$('#'+ element +' > div').removeClass('ui-corner-left');
	$('#'+ element +' > div').removeClass('ui-corner-right');
	if(score<10){
		$('#'+ element +' > div').css({ 'background': 'rgb(255,0,0)' });
	}else if(score<35){
		$('#'+ element +' > div').css({ 'background': 'rgb(255,148,40)'});
	}else if(score<65){
		$('#'+ element +' > div').css({ 'background': 'rgb(160,66,255)'});
	}else if(score<85){
		$('#'+ element +' > div').css({ 'background': 'rgb(0,128,255)'});
	}else{
		$('#'+ element +' > div').css({ 'background': 'rgb(158,255,62)'});
	}
}
