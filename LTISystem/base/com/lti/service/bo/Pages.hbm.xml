<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.lti.service.bo">
	<class
		name="Pages"
		table="ltisystem_pages"
	>
		<id
			name="ID"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="org.hibernate.id.IncrementGenerator"/>
		</id>

		<property
			name="PageName"
			column="PageName"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Symbol"
			column="Symbol"
			type="string"
			not-null="false"
		/>
											
	</class>	
</hibernate-mapping>