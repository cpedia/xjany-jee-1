<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.lti.service.bo">
	<class
		name="Strategy"
		table="ltisystem_strategy"
	>
		<id
			name="ID"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="native"/>
		</id>

		<property
			name="Name"
			column="Name"
			type="string"
			not-null="false"
		/>
		<property
			name="UserName"
			column="UserName"
			type="string"
			not-null="false"
		/>
		<property
			name="UserContent"
			column="UserContent"
			type="string"
			not-null="false"
		/>
		<property
			name="Ticker"
			column="Ticker"
			type="string"
			not-null="false"
		/>
		<property
			name="StrategyClassID"
			column="StrategyClassID"
			type="long"
			not-null="false"
			length="11"
		/>
		<property
			name="Description"
			column="Description"
			type="string"
			not-null="false"
		/>
		<property
			name="Reference"
			column="Reference"
			type="string"
			not-null="false"
		/>
		<property
			name="UserID"
			column="UserID"
			type="java.lang.Long"
			not-null="false"
			length="20"
		/>
		<property
			name="Attributes"
			column="Attributes"
			type="com.lti.type.StringToMap"
			not-null="false"
		/>
		<property
			name="PostID"
			column="PostID"
			type="java.lang.Long"
			not-null="false"
			length="10"
		/>
		<property
			name="ForumID"
			column="ForumID"
			type="java.lang.Long"
			not-null="false"
			length="11"
		/>
		<property
			name="Categories"
			column="Categories"
			type="string"
			not-null="false"
		/>
		<property
			name="ShortDescription"
			column="ShortDescription"
			type="string"
			not-null="false"
		/>
		<property
			name="SimilarIssues"
			column="SimilarIssues"
			type="string"
			not-null="false"
		/>
		<property
			name="Type"
			column="Type"
			type="long"
			not-null="true"
			length="10"
		/>
		<property
			name="MainStrategyID"
			column="MainStrategyID"
			type="java.lang.Long"
			not-null="false"
			length="20"
		/>
		<property
			name="Status"
			column="Status"
			type="int"
		/>
		<property
			name="PlanType"
			column="PlanType"
			type="int"
		/>
		<property
			name="createdDate"
			column="createddate"
			type="java.util.Date"
			not-null="false"
		/>
		<property
			name="ThirdParty"
			column="ThirdParty"
			type="string"
			not-null="false"
		/>
	</class>	
</hibernate-mapping>