<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
"http://www.springframework.org/dtd/spring-beans.dtd">

<beans default-lazy-init="false" default-autowire="autodetect" default-dependency-check="none">
	<bean name="/jsp/admin/customizepage/Main" class="com.lti.action.admin.customizepage.MainAction"
		singleton="false">
		<property name="customizePageManager">
			<ref bean="customizePageManager"/>
		</property>
	</bean>

		
	<bean name="/jsp/admin/customizepage/Save" class="com.lti.action.admin.customizepage.SaveAction"
		singleton="false">
		<property name="customizePageManager">
			<ref bean="customizePageManager"/>
		</property>
	</bean>	

	
</beans>
