<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.lti.service.bo">
	<class
		name="CustomizePage"
		table="ltisystem_customize_page"
	>
		<id
			name="ID"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="native"/>
		</id>

		<property
			name="Name"
			column="name"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Code"
			column="code"
			type="string"
			not-null="false"
			length="100"
		/>

		<property
			name="Template"
			column="template"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Functions"
			column="functions"
			type="string"
			not-null="false"
		/>
											
	</class>	
</hibernate-mapping>