}

function edit(id){
	clear_mce();
	editing=true;
	$('#'+id).val($('#'+id+'_span').html());
	$('#'+id).show();
	$('#'+id+'_span').hide();
	tinyMCE.execCommand('mceAddControl', false, id);  	
}

