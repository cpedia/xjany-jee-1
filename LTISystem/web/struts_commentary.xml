<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="com.lti.action.commentary" namespace="/jsp/commentary"
	extends="struts-default">
		<global-results>
			<result name="exception">/jsp/exception.jsp</result>
		</global-results>
		<global-exception-mappings>
			<exception-mapping result="exception" exception="java.lang.Exception">
			</exception-mapping>
		</global-exception-mappings>
		<action name="Main" class="/jsp/commentary/Main">
			<result name="success">/jsp/commentary/main.jsp</result>
			<result name="error">/jsp/error.jsp</result>
		</action>
	</package>
</struts>