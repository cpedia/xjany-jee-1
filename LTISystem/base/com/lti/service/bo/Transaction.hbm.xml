<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.lti.service.bo">
	<class
		name="Transaction"
		table="ltisystem_transaction"
	>
		<id
			name="ID"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="native"/>
		</id>

		<property
			name="Operation"
			column="OPERATION"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="AssetName"
			column="ASSETNAME"
			type="string"
			not-null="false"
			length="50"
		/>		
		<property
			name="Date"
			column="DATE"
			type="timestamp"
			not-null="false"
			length="19"
		/>
		<property
			name="Amount"
			column="AMOUNT"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="SecurityID"
			column="SECURITYID"
			type="java.lang.Long"
			not-null="false"
		/>
		<property
			name="StrategyID"
			column="StrategyID"
			type="java.lang.Long"
			not-null="false"
		/>
		<property
			name="Symbol"
			column="Symbol"
			type="string"
			not-null="false"
		/>				
		<property
			name="Percentage"
			column="Percentage"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>		
		<property
			name="PortfolioID"
			column="PORTFOLIOID"
			type="java.lang.Long"
			not-null="false"
		/>
		<property
			name="TransactionType"
			column="TransactionType"
			type="java.lang.Integer"
			not-null="false"
		/>
		<property
			name="Share"
			column="Share"
			type="java.lang.Double"
			not-null="false"
		/>
		<property
			name="IsIgnore"
			column="IsIgnore"
			type="java.lang.Boolean"
		/>
	</class>	
</hibernate-mapping>