<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.lti.service.bo">
	<class
		name="VariableFor401k"
		table="ltisystem_variables401k"
	>
		<id
			name="ID"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="native"/>
		</id>

		<property
			name="StrategyID"
			column="StrategyID"
			type="java.lang.Long"
			not-null="true"
			length="20"
		/>
		<property
			name="assetClassName"
			column="AssetClassName"
			type="java.lang.String"
			not-null="false"
			length="200"
		/>		
		<property
			name="symbol"
			column="Symbol"
			type="java.lang.String"
			not-null="false"
			length="200"
		/>
		<property
			name="name"
			column="Name"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="redemption"
			column="Redemption"
			type="java.lang.Integer"
			not-null="false"
		/>
		<property
			name="roundtripLimit"
			column="RoundtripLimit"
			type="java.lang.Integer"
			not-null="false"
		/>
		<property
			name="waitingPeriod"
			column="WaitingPeriod"
			type="java.lang.Integer"
			not-null="false"
		/>
		<property
			name="description"
			column="Description"
			type="java.lang.String"
			not-null="false"
			length="200"
		/>	
		<property
			name="memo"
			column="Memo"
			type="java.lang.String"
			not-null="false"
		/>
		<property
			name="portfolioID"
			column="portfolioID"
			type="java.lang.Long"
			not-null="false"
			length="20"
		/>
		<property
			name="portfolioName"
			column="portfolioName"
			type="java.lang.String"
			not-null="false"
			length="100"
		/>
	</class>	
</hibernate-mapping>