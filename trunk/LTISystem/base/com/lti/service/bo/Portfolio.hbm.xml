<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.lti.service.bo">
	<class
		name="Portfolio"
		table="ltisystem_portfolio"
	>
		<id
			name="ID"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="native"/>
		</id>

		<property
			name="Name"
			column="NAME"
			type="string"
			not-null="false"
		/>
		<property
			name="Description"
			column="Description"
			type="string"
			not-null="false"
		/>
		<property
			name="UserID"
			column="USERID"
			type="java.lang.Long"
			not-null="false"
			length="20"
		/>
		<property
			name="MainStrategyID"
			column="MAINSTRATEGYID"
			type="java.lang.Long"
			not-null="false"
			length="20"
		/>
		<property
			name="EndDate"
			column="EndDate"
			type="java.util.Date"
			not-null="false"
			length="19"
		/>
		<property
			name="OriginalPortfolioID"
			column="OriginalPortfolioID"
			type="java.lang.Long"
			not-null="false"
			length="20"
		/>
		<property
			name="StartingDate"
			column="StartingDate"
			type="java.util.Date"
			not-null="false"
			length="19"
		/>
		<property
			name="Categories"
			column="Categories"
			type="string"
			not-null="false"
			length="256"
		/>
		<property
			name="LastTransactionDate"
			column="lastTransactionDate"
			type="timestamp"
			not-null="false"
			length="19"
		/>
		<property
			name="UserName"
			column="UserName"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="DelayLastTransactionDate"
			column="DelayLastTransactionDate"
			type="timestamp"
			not-null="false"
			length="19"
		/>
		<property
			name="Type"
			column="Type"
			type="java.lang.Long"
			not-null="false"
			length="20"
		/>
		<property
			name="Attributes"
			column="Attributes"
			type="com.lti.type.StringToMap"
			not-null="false"
		/>
		<property
			name="Strategies"
			column="Strategies"
			type="com.lti.type.StringToStrategyInf"
			not-null="false"
		/>
		<property
			name="State"
			column="State"
			type="java.lang.Integer"
			not-null="true"
		/>
		<property
			name="createdDate"
			column="createddate"
			type="java.util.Date"
			not-null="false"
		/>
	</class>	
</hibernate-mapping>