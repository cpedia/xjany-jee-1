<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.lti.service.bo">
	<class
		name="Company"
		table="ltisystem_company"
	>
		<id
			name="ID"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="native"/>
		</id>
		<property
			name="Symbol"
			column="Symbol"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Sector"
			column="Sector"
			type="string"
			not-null="false"
			length="100"
		/>	    
		<property
			name="Industry"
			column="Industry"
			type="string"
			not-null="false"
			length="100"
		/>			

	</class>	
</hibernate-mapping>