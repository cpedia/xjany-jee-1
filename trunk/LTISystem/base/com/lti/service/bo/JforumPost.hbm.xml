<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.lti.service.bo">
	<class
		name="JforumPost"
		table="jforum_posts"
	>
		<id
			name="PostID"
			type="java.lang.Integer"
			column="post_id"
		>
			<generator class="native"/>
		</id>
		<property
			name="TopicID"
			column="topic_id"
			type="java.lang.Integer"
		/>
		<property
			name="ForumID"
			column="forum_id"
			type="java.lang.Integer"
		/>
		<property
			name="UserID"
			column="user_id"
			type="java.lang.Integer"
		/>
		<property
			name="PostTime"
			column="post_time"
			type="java.sql.Timestamp"
		/>
		<property
			name="PosterIP"
			column="poster_ip"
			type="java.lang.String"
		/>
		<property
			name="EnableBBCode"
			column="enable_bbcode"
			type="java.lang.Boolean"
		/>		
		<property
			name="EnableHtml"
			column="enable_html"
			type="java.lang.Boolean"
		/>		
		<property
			name="EnableSmilies"
			column="enable_smilies"
			type="java.lang.Boolean"
		/>	
		<property
			name="EnableSig"
			column="enable_sig"
			type="java.lang.Boolean"
		/>	
		<property
			name="PostEditTime"
			column="post_edit_time"
			type="java.sql.Timestamp"
		/>
		<property
			name="PostEditCount"
			column="post_edit_count"
			type="java.lang.Integer"
		/>
		<property
			name="Status"
			column="status"
			type="java.lang.Boolean"
		/>	
		<property
			name="Attach"
			column="attach"
			type="java.lang.Boolean"
		/>	
		<property
			name="NeedModerate"
			column="need_moderate"
			type="java.lang.Boolean"
		/>	
		<property
			name="PlanID"
			column="plan_id"
			type="java.lang.String"
		/>
	</class>	
</hibernate-mapping>