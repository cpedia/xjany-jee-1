<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.lti.service.bo">
	<class
		name="IncomeStatement"
		table="ltisystem_incomestatement"
	>
		<id
			name="ID"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="native"/>
		</id>

		<property
			name="Symbol"
			column="Symbol"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Date"
			column="Date"
			type="timestamp"
			not-null="false"
			length="19"
		/>
		<property
			name="Year"
			column="Year"
			type="integer"
			not-null="false"
			length="11"
		/>	
		<property
			name="Quarter"
			column="Quarter"
			type="integer"
			not-null="false"
			length="11"
		/>	
		<property
			name="Revenue"
			column="Revenue"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="COGS"
			column="COGS"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="SGNA"
			column="SGNA"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="RND"
			column="RND"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="OtherOperExp"
			column="OtherOperExp"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		
		<property
			name="TotalOtherInc"
			column="TotalOtherInc"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		
		<property
			name="IntExp"
			column="IntExp"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="AcctChange"
			column="AcctChange"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="DiscOper"
			column="DiscOper"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="ExtItem"
			column="ExtItem"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="IncomeTax"
			column="IncomeTax"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="Shares"
			column="Shares"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="Floats"
			column="Floats"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>	
		<property
			name="InstitutionHolder"
			column="InstitutionHolder"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>	
		<property
			name="GrossProfit"
			column="GrossProfit"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="TotalOperExp"
			column="TotalOperExp"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="OperInc"
			column="OperInc"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="PreTaxInc"
			column="PreTaxInc"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="NetInc"
			column="NetInc"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="AfterTaxInc"
			column="AfterTaxInc"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>					
											
	</class>	
</hibernate-mapping>