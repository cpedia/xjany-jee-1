<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.lti.service.bo">
	<class name="PlanScore" table="ltisystem_planscore">
		<id
			name="ID"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="native"/>
		</id>

		<property
			name="PlanID"
			column="PlanID"
			type="java.lang.Long"
			not-null="true"
		/>
		<property
			name="PlanName"
			column="PlanName"
			type="string"
			not-null="true"
		/>
		<property
			name="EndDate"
			column="EndDate"
			type="timestamp"
			not-null="false"
		/>
		<property
			name="SAAScore"
			column="SAAScore"
			type="java.lang.Double"
			not-null="false"
		/>
		<property
			name="TAAScore"
			column="TAAScore"
			type="java.lang.Double"
			not-null="false"
		/>
		<property
			name="SAAReturn"
			column="SAAReturn"
			type="java.lang.Double"
			not-null="false"
		/>
		<property
			name="TAAReturn"
			column="TAAReturn"
			type="java.lang.Double"
			not-null="false"
		/>
		<property
			name="CoverageScore"
			column="CoverageScore"
			type="java.lang.Double"
			not-null="false"
		/>	
		<property
			name="FundQualityScore"
			column="FundQualityScore"
			type="java.lang.Double"
			not-null="false"
		/>	
		<property
			name="CapabilityScore"
			column="CapabilityScore"
			type="java.lang.Double"
			not-null="false"
		/>	
		<property
			name="InvestmentScore"
			column="InvestmentScore"
			type="java.lang.Double"
			not-null="false"
		/>
		<property
			name="Status"
			column="Status"
			type="java.lang.Integer"
			not-null="false"
		/>
		<property
			name="CoverageValue"
			column="CoverageValue"
			type="java.lang.Double"
			not-null="false"
		/>	
		<property
			name="FundQualityValue"
			column="FundQualityValue"
			type="java.lang.Double"
			not-null="false"
		/>	
		<property
			name="CapabilityValue"
			column="CapabilityValue"
			type="java.lang.Double"
			not-null="false"
		/>	
		<property
			name="InvestmentValue"
			column="InvestmentValue"
			type="java.lang.Double"
			not-null="false"
		/>
		<property
			name="MajorAssetClass"
			column="MajorAssetClass"
			type="java.lang.Integer"
			not-null="false"
		/>
	</class>	
</hibernate-mapping>