<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.lti.service.bo">
	<class name="WidgetUser" table="ltisystem_widgetuser">
	
		<id	name="ID" type="java.lang.Long"	column="ID"	>
			<generator class="native"/>
		</id>
		<property name="email" column="email" type="string" not-null="false" length="45" />
		<property name="website" column="website" type="string" not-null="false" length="45" />
		
	</class>	
</hibernate-mapping>