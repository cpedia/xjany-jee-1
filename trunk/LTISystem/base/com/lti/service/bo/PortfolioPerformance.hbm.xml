<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.lti.service.bo">
	<class name="PortfolioPerformance" table="ltisystem_portfolioperformance">
		<id
			name="ID"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="native"/>
		</id>

		<property
			name="PlanID"
			column="PlanID"
			type="java.lang.Long"
			not-null="true"
		/>
		
		<property
			name="PortfolioID"
			column="PortfolioID"
			type="java.lang.Long"
			not-null="true"
		/>
		
		<property
			name="ARRating"
			column="ARRating"
			type="java.lang.Double"
			not-null="false"
		/>
		<property
			name="SortinoRating"
			column="SortinoRating"
			type="java.lang.Double"
			not-null="false"
		/>
		<property
			name="SharpeRating"
			column="SharpeRating"
			type="java.lang.Double"
			not-null="false"
		/>
		<property
			name="TreynorRating"
			column="TreynorRating"
			type="java.lang.Double"
			not-null="false"
		/>
		<property
			name="DrawdownRating"
			column="DrawdownRating"
			type="java.lang.Double"
			not-null="false"
		/>	
		<property
			name="WinningRating"
			column="WinningRating"
			type="java.lang.Double"
			not-null="false"
		/>	
	</class>	
</hibernate-mapping>