<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.lti.service.bo">
	<class
		name="RegionCustomize"
		table="ltisystem_region_customize"
	>
		<id
			name="ID"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="org.hibernate.id.IncrementGenerator"/>
		</id>

		<property
			name="PageName"
			column="page_name"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="RegionName"
			column="region_name"
			type="string"
			not-null="false"
		/>

		<property
			name="RegionContent"
			column="region_content"
			type="string"
			not-null="false"
		/>

		<property
			name="GroupID"
			column="group_id"
			type="java.lang.Long"
			not-null="false"
		/>
		
											
	</class>	
</hibernate-mapping>