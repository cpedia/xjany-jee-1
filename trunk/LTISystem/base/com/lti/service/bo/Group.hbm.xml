<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.lti.service.bo">
	<class
		name="Group"
		table="ltisystem_group"
	>
		<id
			name="ID"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="native"/>
		</id>

		<property
			name="Name"
			column="Name"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Description"
			column="Description"
			type="string"
			not-null="false"
			length="100"
		/>

											
	</class>	
</hibernate-mapping>