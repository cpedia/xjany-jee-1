		length="16"
	/>	
	<property
		name="AssetClassID"
		column="AssetClassID"
		type="java.lang.Long"
		not-null="false"
		length="16"
	/>	
	<property
		name="SecurityName"
		column="SecurityName"
		type="java.lang.String"
		not-null="false"
		length="200"
	/>
	<property
		name="Year"
		column="Year"
		type="java.lang.Long"
