<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="com.lti.action.admin.quartz" namespace="/jsp/admin/quartz" extends="struts-default"> 
		<global-results>
			<result name="exception">/jsp/admin/exception.jsp</result>
		</global-results>
		<global-exception-mappings>
			<exception-mapping result="exception" exception="java.lang.Exception"></exception-mapping>
		</global-exception-mappings>
		
		<action name="Save" class="/jsp/admin/quartz/Save">
			<result name="input">/jsp/admin/quartz/save.jsp</result>
			<result name="success">/jsp/admin/info.jsp</result>
			<result name="error">/jsp/admin/error.jsp</result>
		</action>
	</package>
</struts>
