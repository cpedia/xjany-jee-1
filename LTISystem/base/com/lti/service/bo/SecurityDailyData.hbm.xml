<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.lti.service.bo">
	<class
		name="SecurityDailyData"
		table="ltisystem_securitydailydata"
		batch-size="200"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="ID"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="native"/>
		</id>

		<property
			name="Date"
			column="DATE"
			type="timestamp"
			not-null="false"
			length="19"
		/>
		<property
			name="Close"
			column="Close"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="Split"
			column="SPLIT"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="Dividend"
			column="DIVIDEND"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="Open"
			column="OPEN"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="High"
			column="HIGH"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="AdjClose"
			column="ADJCLOSE"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="Low"
			column="LOW"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="ReturnDividend"
			column="RETURNDIVIDEND"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="TurnoverRate"
			column="TURNOVERRATE"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="EPS"
			column="EPS"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="PE"
			column="P_E"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="MarketCap"
			column="MARKETCAP"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>
		<property
			name="SecurityID"
			column="SECURITYID"
			type="java.lang.Long"
			not-null="false"
		/>
		<property
			name="Volume"
			column="VOLUME"
			type="java.lang.Long"
			not-null="false"
		/>
		<property
			name="NAV"
			column="NAV"
			type="java.lang.Double"
			not-null="false"
		/>
		<property
			name="AdjNAV"
			column="ADJNAV"
			type="java.lang.Double"
			not-null="false"
		/>	
	</class>	
</hibernate-mapping>