<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.lti.service.bo">
	<class
		name="Indicator"
		table="ltisystem_indicator"
	>
		<id
			name="ID"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="native"/>
		</id>
		<property
			name="Symbol"
			column="Symbol"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Description"
			column="description"
			type="string"
			not-null="false"
			length="200"
		/>


	</class>	
</hibernate-mapping>