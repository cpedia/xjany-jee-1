<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.lti.service.bo">
	<class
		name="JforumTopics"
		table="jforum_topics"
	>
		<id
			name="ID"
			type="java.lang.Integer"
			column="topic_id"
		>
			<generator class="native"/>
		</id>

		<property
			name="ForumId"
			column="forum_id"
			type="java.lang.Integer"
		/>
		<property
			name="TopicTitle"
			column="topic_title"
			type="java.lang.String"
		/>
		<property
			name="UserId"
			column="user_id"
			type="java.lang.Integer"
		/>
		<property
			name="TopicTime"
			column="topic_time"
			type="java.sql.Timestamp"
		/>
		<property
			name="TopicViews"
			column="topic_views"
			type="java.lang.Integer"
		/>
		<property
			name="TopicReplies"
			column="topic_replies"
			type="java.lang.Integer"
		/>
		<property
			name="TopicStatus"
			column="topic_status"
			type="java.lang.Integer"
		/>
		<property
			name="TopicVoteId"
			column="topic_vote_id"
			type="java.lang.Integer"
		/>
		<property
			name="TopicType"
			column="topic_type"
			type="java.lang.Integer"
		/>
		<property
			name="TopicFirstPostId"
			column="topic_first_post_id"
			type="java.lang.Integer"
		/>
		<property
			name="TopicLastPostId"
			column="topic_last_post_id"
			type="java.lang.Integer"
		/>
		<property
			name="TopicMovedId"
			column="topic_moved_id"
			type="java.lang.Integer"
		/>
		<property
			name="Moderated"
			column="moderated"
			type="java.lang.Integer"
		/>
		<property
			name="PlanId"
			column="plan_id"
			type="java.lang.String"
		/>
		<property
			name="NeedModerate"
			column="need_moderate"
			type="boolean"
		/>
	</class>	
</hibernate-mapping>